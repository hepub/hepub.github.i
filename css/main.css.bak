  text-shadow: 0 1px 0 rgba(20, 20, 20, 0.4);
  -webkit-box-shadow: inset 0 0 0 1px rgba(20, 20, 25, 0.08);
  box-shadow: inset 0 0 0 1px rgba(20, 20, 25, 0.08);
  padding: 0 10px;
}

.footer .ft_nav .col_nav .box_submit {
  padding-top: 10px;
}

.footer .ft_nav .col_nav .box_submit .btn_submit {
  font-size: 14px;
  color: #daeaf6;
  background: #81b4da;
  border: 1px solid #6F90AD;
  -webkit-box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.3), 0 4px 3px -3px rgba(20, 20, 25, 0.2);
  box-shadow: inset 0 1px 0 rgba(255, 255, 255, 0.3), 0 4px 3px -3px rgba(20, 20, 25, 0.2);
  border-radius: 3px;
  padding: 3px 10px;
  margin-right: 20px;
  -webkit-transition: all 0.4s ease;
  transition: all 0.4s ease;
}

.footer .ft_nav .col_nav .box_submit .btn_submit:hover {
  background: #5a9dce;
}

.footer .ft_nav .col_nav .box_submit .txt_hint {
  font-size: 14px;
  color: #386a8d;
}

.footer .ft_sub_nav {
  margin-bottom: 20px;
}

.footer .ft_sub_nav ul li {
  position: relative;
  float: left;
  padding-right: 10px;
  margin-right: 10px;
}

.footer .ft_sub_nav ul li:last-child {
  padding-right: 0 !important;
  margin-right: 0 !important;
}

.footer .ft_sub_nav ul li:last-child:before {
  display: none;
}

.footer .ft_sub_nav ul li:before {
  content: '|';
  position: absolute;
  right: 0;
  top: 0;
  font-size: 14px;
  color: #fff;
}

.footer .ft_sub_nav ul li a {
  font-size: 14px;
  color: #daeaf6;
  text-shadow: 0 1px 0 rgba(20, 20, 20, 0.4);
  text-decoration: none;
  -webkit-transition: all 0.4s ease;
  transition: all 0.4s ease;
}

.footer .ft_sub_nav ul li a:hover {
  color: #59df3e;
  text-shadow: none;
}

.footer .ft_copyright p {
  font-size: 14px;
  color: #386a8d;
  margin-bottom: 5px;
}

.footer .ft_copyright p a {
  color: #daeaf6;
  text-shadow: 0 1px 0 rgba(20, 20, 20, 0.4);
  text-decoration: none;
  -webkit-transition: all 0.4s ease;
  transition: all 0.4s ease;
}

.footer .ft_copyright p a:hover {
  color: #59df3e;
  text-shadow: none;
}
